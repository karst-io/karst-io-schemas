{
	"name": "Entrance",
	"type": "record",
	"namespace": "io.karst.schemas",
	"fields": [
		{"name": "name", "type": "string"},
		{"name": "uuid", "type": "string"},
		{"name": "revision", "type": "int"},

		{"name": "entry_status", "type":{
			"name": "EntryStatus",
			"type": "enum",
			"symbols": ["UNKNOWN","OPEN","DESTROYED","LOCKED / GATED","FORBIDDEN","PERMISSION REQUIRED","PERMIT OR WAIVER REQUIRED"]
		}},
		{"name": "entry_status_ext", "type": "string"},
		
		{"name": "entry_ownership", "type": {
			"name": "EntryOwnership",
			"type": "enum",
			"symbols": ["UNKNOWN","COMMERCIAL","GOVERNMENT","PRIVATE PROPERTY"]
		}},
		{"name": "entry_ownership_ext", "type": "string"},
		
		{"name": "entrance_type", "type": {
			"name":"EntranceType",
			"type": "enum",
			"symbols": ["UNKNOWN", "HORIZONTAL", "VERTICAL"]
		}},
		{"name": "entrance_type_ext", "type":"string"},

		{"name": "entrance_negotiation", "type": {
			"name": "EntranceNegotiation",
			"type": "enum",
			"symbols": ["UNKNOWN", "RAPPEL", "CLIMB", "WALK", "STOOP", "CRAWL", "SWIM"]
		}},
		{"name": "entrance_negotiation_ext", "type":"string"},

		{"name": "entrance_ldim", "type": "int"},
		{"name": "entrance_sdim", "type": "int"},

		{"name": "entrance_formation", "type":  {
			"name": "EntranceFormation",
			"type": "enum",
			"symbols": ["UNKNOWN", "NATURAL", "ARTIFICIAL"]
		}},
		{"name": "entrance_formation_ext", "type":"string"},

		{"name": "entrance_geoformation", "type": "string"},

		{"name": "entrance_location", "type": {
			"name": "EntranceLocation",
			"type": "enum",
			"symbols": ["UNKNOWN", "CLIFF", "BLUFF OR HEADWALL", "BENCH", "OUTCROP", "STREAMBED", "NO INDICATION"]
		}},
		{"name": "entrance_location_ext", "type":"string"},

		{"name": "entrance_characteristics", "type": {
			"name": "EntranceCharacteristics",
			"type": "enum",
			"symbols": ["UNKNOWN", "NONE", "CRACK", "SHELF OR LEDGE", "SINK", "WINDOW"]
		}},
		{"name": "entrance_characteristics_ext", "type":"string"},

		{"name": "entrance_hydrology", "type":  {
			"name": "EntranceHydrology",
			"type": "enum",
			"symbols": ["UNKNOWN", "NONE", "SUBMERGED", "WATER (IN)", "WATER (OUT)"]
		}},
		{"name": "entrance_hydrology_ext", "type":"string"},

		{"name": "entrance_hazards", "type":  {
			"name": "EntranceHazards",
			"type": "enum",
			"symbols": ["UNKNOWN", "NONE", "UNSTABLE", "WATER", "ORGANIC", "ANIMAL", "HUMAN", "CHEMICAL"]
		}},
		{"name": "entrance_hazards_ext", "type":"string"},
		
		{"name": "location_datum", "type": "string"},
		{"name": "location_lat", "type": "float"},
		{"name": "location_long", "type": "float"},

		{"name": "entrance_photos", "type": {
				"type": "array",
				"items": "io.karst.schemas.Image"
		}}
	]
}